@model ContactModel

<div class="row">
	<div class="col-sm-3">
		<div class="companyinfo">
			<p><img src="/media/logo/@Model.LogoImg" width="140" height="150" /></p>
			<p>@Html.Raw(@Model.Description)</p>
		</div>
	</div>
	<div class="col-sm-4 col-md-4">
            @Html.Raw(@Model.Map)
		
	</div>
	<div class="col-sm-3" style="margin-left: 150px;">
		<div class="address">
			<h5 style="margin-left: 120px;"><b>Name: @Model.Name</b></h5>
			<h5 style="margin-left: 120px;">Email: @Model.Email</h5>
			<h5 style="margin-left: 120px;">Hotline: @Model.Phone</h5>
		</div>
	</div>
	
</div>